modeh(initiatedAt(meeting(+person,+person),+time))
modeh(terminatedAt(meeting(+person,+person),+time))

modeb(happensAt(walking(+person),+time))
modeb(happensAt(active(+person),+time))
modeb(happensAt(inactive(+person),+time))
modeb(happensAt(abrupt(+person),+time))
modeb(happensAt(running(+person),+time))

%modeb(not happensAt(walking(+person),+time))
%modeb(not happensAt(active(+person),+time))
%modeb(not happensAt(inactive(+person),+time))
%modeb(not happensAt(abrupt(+person),+time))
%modeb(not happensAt(running(+person),+time))

modeb(happensAt(appear(+person),+time))
modeb(happensAt(disappear(+person),+time))
modeb(not happensAt(disappear(+person),+time))
modeb(close(+person,+person,#dist,+time))
modeb(far(+person,+person,#dist,+time))

examplePattern(holdsAt(meeting(+person,+person),+time))



